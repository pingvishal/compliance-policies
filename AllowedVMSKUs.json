{
    "if": {
      "allOf": [
        {
          "field": "type",
          "equals": "Microsoft.Web/serverfarms"
        },
        {
          "field": "Microsoft.Web/serverfarms/properties.tags",
          "contains": "dev"
        },
        {
          "not": {
            "field": "Microsoft.Web/serverfarms/sku.name",
            "in": [
              "B1",
              "A1"
            ]
          }
        }
      ]
    },
    "then": {
      "effect": "audit"
    }
}
